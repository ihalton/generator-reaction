{
  "name": "generator-reaction-next",
  "version": "0.0.0",
  "description": "Yeoman generator for Reaction NEXT JavaScript services.",
  "homepage": "https://github.com/rectioncommerce/generator-reaction-next",
  "author": {
    "name": "Ticean Bennett",
    "email": "ticean@reactioncommerce.com",
    "url": "https://reactioncommerce.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "reaction",
    "reactioncommerce",
    "reaction-next",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "rimraf": "^2.6.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "git@github.com:reactioncommerce/reaction-next.git",
  "license": "GPL-3.0"
}
